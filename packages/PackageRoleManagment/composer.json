{
  "name": "hip/package-role-management",
  "description": "Managment for roles in administrator application.",
  "authors": [
    {
      "name": "George Zampetakis",
      "email": "george.zampetakis@syneru.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/contracts": "^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Hip\\PackageRoleManagement\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hip\\PackageRoleManagement\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "Hip\\PackageRoleManagement\\Providers\\RolesServiceProvider"
      ]
    }
  }
}
