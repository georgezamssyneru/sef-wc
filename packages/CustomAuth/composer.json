{
    "name": "hip/custom-auth",
    "description": "Custom Authorization .",
    "authors": [
        {
            "name": "George Zampetakis",
            "email": "george.zampetakis@syneru.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Hip\\CustomAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hip\\CustomAuth\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Hip\\CustomAuth\\Providers\\CustomAuthServiceProvider"
            ]
        }
    }
}
