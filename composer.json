{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "barryvdh/laravel-dompdf": "^2.0",
    "casbin/laravel-authz": "^3.1",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "hip/custom-auth": "dev-main",
    "hip/package-role-management": "dev-main",
    "hip/package-user-management": "dev-main",
    "krlove/eloquent-model-generator": "*",
    "laravel/framework": "^8.65",
    "laravel/sanctum": "^2.13",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^3.4",
    "php-amqplib/php-amqplib": "^3.2.0",
    "phpseclib/phpseclib": "~3.0",
    "symfony/css-selector": "^6.0",
    "vladimir-yuldashev/laravel-queue-rabbitmq": "^11.3",
    "doctrine/dbal": "^3.0",
    "yajra/laravel-oci8": "^8.0"
  },
  "require-dev": {
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "laravel/breeze": "^1.6",
    "laravel/dusk": "^6.25",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^5.10",
    "phpunit/phpunit": "^9.5.10",
    "staudenmeir/dusk-updater": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "./packages/CustomAuth"
    },
    {
      "type": "path",
      "url": "./packages/PackageUserManagment"
    },
    {
      "type": "path",
      "url": "./packages/PackageRoleManagment"
    }
  ],
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
